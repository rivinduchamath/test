<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.foodieapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AManage" />
        <activity android:name=".Manage" />
        <activity android:name=".SignUp" />
        <activity android:name=".CProfile" />
        <activity android:name=".TopEats" />
        <activity android:name=".TopCategories" />
        <activity android:name=".SpecialOffers" />
        <activity android:name=".CManage" />
        <activity android:name=".Favorites" />
        <activity android:name=".Diatery" />
        <activity android:name=".OurMenu" />
        <activity android:name=".LatestDeals" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>